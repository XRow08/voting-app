{
  "compilerOptions": {
    "baseUrl": ".", 
    "paths": {
      "@components/*": ["./components/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/starsOff/Footer.jsx", "components/starsOff/Infosection.jsx", "components/starsOff/ProgressBar.jsx", "components/starsOff/Starswitch.jsx", "components/starsOff/VotingArea.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
